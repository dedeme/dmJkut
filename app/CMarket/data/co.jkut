/// Company data.

/// Constructor
/// \s, b -> m<co>
mk = \nick, isSelected -> return {nick, isSelected};;

/// \<co> -> [*...]
toJs = \C -> return [C.nick, C.isSelected];;

/// \[*...] -> s
fromJs = \A -> return mk(A[0], A[1]);;
