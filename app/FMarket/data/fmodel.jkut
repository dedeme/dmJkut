/// Flea model.

/// Constructor
///   modelId: Model identifier.
///   Params : Model parameters.
/// \s, [f...] -> <fmodel>
mk = \modelId, Params -> return {modelId, Params};;

/// \[*(JS)...] -> <fmodel>
fromJs = \A -> return mk(A[0], A[1]);;
