/// Interface for problem editors.

/// Set active value.
/// In 'annsEditor' and 'delEditor'.
/// \(<annsEditor> | <delEditor>), b -> ()
active = \ed, value -> ed.active(value);;

/// Deactivate editor.
/// In 'del', 'modify' and 'newEntry'.
/// \(<delEditor> | <modify> | <newEntry>) -> ()
deactivate = \ed -> ed.deactivate();;

/// Update account 'acc' with editor data.
/// In 'annsEditor', 'modify' and 'newEntry'.
/// \(<annsEditor> | <modify> | <newEntry>), s -> ()
updateAccount = \ed, acc -> ed.updateAccount(acc);;

