/// Diary entry data.

/// Constructor
///   date: Operation date.
///   description:  Description of operation.
///   debits: Dictionary {DebitsCounts: values (currency)}.
///   credits: Dictionary {CreditsCounts: values (currency)}.
/// \n,s,{n.},{n.} -> <diaryEntry>
mk : date, description, debits, credits;

/// \<myType> -> [*.]
toJs = \o -> return [
    time.toStr(o[date]),
    o[description],
    o[debits],
    o[credits]
  ];;

/// \[*.] -> <myType>
fromJs = \A -> return [
    time.fromStr(A[date])!,
    A[description],
    A[debits],
    A[credits]
  ];;
