/// Hot dic of a model.

import "data/paramsEval";

/// Returns a dic with the following values.
///   date: Date of data.
///   entries: Parameters-Evaluations of a model.
/// \s, a[<paramsEval>...] -> d<HotMap>
mk = \date, Entries -> {
  return {
    date,
    Entries
  };
};

/// \{date:s, entries: [*(JS)...]} -> <HotMap>
fromJs = \D -> return mk(
    D.date,
    arr.map(D.entries, paramsEval.fromJs)
  );;

