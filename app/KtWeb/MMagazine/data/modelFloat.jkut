/// Pair Model - Float.

/// \s, n -> d<ModelFloat>
mk = \model, value ->  return {
  model,
  value
};;

/// \<ModelValue> -> [*(JS)...]
toJs = \Mf -> return [Mf.model, Mf.value];;

/// \[*(JS)...] -> <ModelValue>
fromJs = \A -> return mk(A[0], A[1]);;
