/// Unsorted model evaluations of a date.

import "data/modelFloat";

/// \s, [<ModelFloat>...] -> d<ModelFloats>
mk = \date, Values -> return {
  date,
  Values
};;

/// \<ModelFloats> -> [*(JS)...]
toJs = \Mf -> return [
  Mf.date,
  arr.map(Mf.Values, modelFloat.toJs)
];;

/// \[*(JS)...] -> <ModelFloats>
fromJs = \A -> return mk(
    A[0],
    arr.map(A[1], modelFloat.fromJs)
  );;
