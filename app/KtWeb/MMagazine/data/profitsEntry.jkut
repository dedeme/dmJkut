/// Profits data of all the investors.

/// Creates a profits entry.
///   \s, n -> d<ProfitsEntry>
mk = \date, profits -> return {
  date,
  profits
};;

/// \<ProfitsEntry> -> [*(JS)...]
toJs = \Pe -> return [Pe.date, Pe.profits];;

/// \[*(JS)...] -> <ProfitsEntry>
fromJs = \A -> return mk(A[0], A[1]);;
