/// Summary value.

/// Constructor.
///   ix : Index in results array.
///   val: Value.
/// \n,n -> <smVal>
mk : ix, val;

/// Returns 'true' if 'v1 < v2'.
/// \<smVal>, <smVal> -> b
less = \v1, v2 -> return v1[val] < v2[val];;
