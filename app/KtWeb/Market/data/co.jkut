/// <co> data definition.

/// Constructor.
///   nick      :
///   isSelected:
/// \s,b -> <co>
mk : nick, isSelected;

/// Set isSelected.
/// \<co>, b -> ()
setSelected = \c, value -> c[isSelected] = value;;
