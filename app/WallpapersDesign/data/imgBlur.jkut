/// Image 'blur' data

/// Creates new 'blur' parameters.
///   ratio: Value between 0 and 100 (both inclusive).
/// \n -> d<imgBlur>
mk = \ratio -> return {ratio};;

/// \<imgBlur> -> *(JS)
toJs = \IB -> return IB.ratio;;

/// \*(JS) -> <imgBlur>
fromJs = \j -> return mk(j);;
